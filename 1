#include <stdio.h>

int main() {
    int num1 = 10;
    float num2 = 3.14;
    char ch = 'A';

    printf("Valores originais:\n");
    printf("Inteiro: %d\n", num1);
    printf("Real: %.2f\n", num2);
    printf("Char: %c\n\n", ch);

    num1 = 20;
    num2 = 2.71828;
    ch = 'B';

    printf("Valores modificados:\n");
    printf("Inteiro: %d\n", num1);
    printf("Real: %.5f\n", num2);
    printf("Char: %c\n", ch);

    return 0;
}
