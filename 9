#include <stdio.h>

void imprimirArray(int *ptrArray, int tamanho) {
    int *ptrFinal = ptrArray + tamanho;

    for (; ptrArray < ptrFinal; ptrArray++) {
        printf("%d ", *ptrArray);
    }

    printf("\n");
}

int main() {
    int array[] = {1, 3, 5, 7, 9};
    int tamanho = sizeof(array) / sizeof(array[0]);

    imprimirArray(array, tamanho);

    return 0;
}
