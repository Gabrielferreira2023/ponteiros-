#include <stdio.h>

int main() {
    int num1, num2;
    int *ptr1, *ptr2;

    num1 = 10;
    num2 = 20;

    ptr1 = &num1;
    ptr2 = &num2;

    if (ptr1 > ptr2) {
        printf("O maior endereço é: %p\n", ptr1);
    } else {
        printf("O maior endereço é: %p\n", ptr2);
    }

    return 0;
}
